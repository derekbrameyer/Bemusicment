<?xml version="1.0" encoding="utf-8"?>
<layer-list
	xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:id="@android:id/background">
		<shape>
			<corners
				android:radius="5dip" />
			<gradient
				android:startColor="@color/vertical_seek_bar_start_color"
				android:centerColor="@color/vertical_seek_bar_center_color"
				android:centerX="0.75"
				android:endColor="@color/vertical_seek_bar_end_color"
				android:angle="0" />
		</shape>
	</item>
	<item
		android:id="@android:id/secondaryProgress">
		<clip
			android:clipOrientation="vertical"
			android:gravity="bottom">
			<shape>
				<corners
					android:radius="5dip" />
				<gradient
					android:startColor="@color/vertical_seek_bar_start_color"
					android:centerColor="@color/vertical_seek_bar_center_color"
					android:centerX="0.75"
					android:endColor="@color/vertical_seek_bar_end_color"
					android:angle="0" />
			</shape>
		</clip>
	</item>
	<item
		android:id="@android:id/progress">
		<clip
			android:clipOrientation="vertical"
			android:gravity="bottom">
			<shape>
				<corners
					android:radius="5dip" />
				<gradient
					android:startColor="@color/vertical_seek_bar_start_color"
					android:centerColor="@color/vertical_seek_bar_center_color"
					android:centerX="0.75"
					android:endColor="@color/vertical_seek_bar_end_color"
					android:angle="0" />
			</shape>
		</clip>
	</item>
</layer-list>